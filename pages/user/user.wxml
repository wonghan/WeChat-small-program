<view class="page">
  <view class="page__hd">
    <view class="avatar">
      <open-data type="userAvatarUrl"></open-data>
    </view>
    <view class="nickName">
      <open-data type="userNickName"></open-data>
    </view>
  </view>
  <view class="page__bd">
      <view class="list">
        <view class="list-item">
           <image class="list-item-img" src="/mocks/img/user-information.png"></image> 
          <view class="list-item-text">我的信息</view>
        </view>
        <view class="list-item">
           <image class="list-item-img" src="/mocks/img/user-watch.png"></image> 
          <view class="list-item-text">我的足迹</view>
        </view>
        <navigator url="/pages/star/star" open-type="navigate">
          <view class="list-item">
            <image class="list-item-img" src="/mocks/img/user-star.png"></image> 
            <view class="list-item-text">我的收藏</view>
          </view>
        </navigator>
        <view class="list-item">
        </view>
    </view>
  </view>
</view>
